pipeline {
    agent any
    stages {
        stage('Example Deploy') {
            when {
                branch 'production'
            }
            steps {
                echo 'Deploying'
            }
        }
        stage('Example Testing'){
            when {
                branch 'qa'
            }
            steps{
                echo 'Testing in progress'
            }
            }
        stage('Example building'){
            when {
                branch 'Dev'
            }
            steps{
                echo 'Build in progress'
            }
            }
    }
}
